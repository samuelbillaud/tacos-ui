{
  "name": "@tacos-ui/panda-preset",
  "version": "0.0.1",
  "description": "Panda CSS Preset for Tacos UI",
  "keywords": [
    "Panda CSS",
    "Preset"
  ],
  "author": "Samuel Billaud <samuel.billaud1990@gmail.com> (https://github.com/samuelbillaud)",
  "license": "MIT",
  "repository": "https://github.com/samuelbillaud/zaatar-ui",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts"
  },
  "dependencies": {
    "@ark-ui/anatomy": "2.1.0",
    "@radix-ui/colors": "^3.0.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.28.0",
    "@pandacss/types": "0.28.0",
    "tsup": "8.0.1"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0",
    "@pandacss/types": ">0.22.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "main": "src/index.ts"
}
