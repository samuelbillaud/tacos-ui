{
  "name": "@tacos-ui/panda-preset",
  "version": "0.0.3",
  "description": "Panda CSS Preset for Tacos UI",
  "keywords": [
    "Panda CSS",
    "Preset"
  ],
  "author": "Samuel Billaud <samuel.billaud1990@gmail.com> (https://github.com/samuelbillaud)",
  "license": "MIT",
  "repository": "https://github.com/samuelbillaud/zaatar-ui",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "studio": "pandacss studio"
  },
  "dependencies": {
    "@ark-ui/anatomy": "3.0.0",
    "@radix-ui/colors": "^3.0.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.37.2",
    "@pandacss/studio": "^0.37.2",
    "@pandacss/types": "0.37.2"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0",
    "@pandacss/types": ">0.22.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts"
}
