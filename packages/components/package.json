{
  "name": "@tacos-ui/react",
  "description": "React component for Tacos UI",
  "version": "0.5.0",
  "type": "module",
  "keywords": [
    "Ark UI",
    "Panda CSS",
    "Preset",
    "Tacos UI"
  ],
  "author": {
    "name": "Samuel Billaud",
    "email": "samuel.billaud1990@gmail.com",
    "url": "https://github.com/samuelbillaud"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelbillaud/tacos-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/components/ui/index.ts",
      "import": {
        "types": "./dist/index.d.dts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "panda codegen",
    "build": "tsup",
    "build-storybook": "storybook build",
    "dev": "storybook dev -p 6006",
    "typecheck": "tsc",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "chromatic": "^11.3.0",
    "@pandacss/dev": "0.42.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "storybook": "^8.1.3",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-tsconfig-paths": "4.3.2"
  },
  "dependencies": {
    "@ark-ui/react": "3.2.1",
    "@tacos-ui/panda-preset": "workspace:*",
    "@tacos-ui/styled-system": "workspace:*",
    "lucide-react": "0.375.0",
    "react": "18.3.1",
    "react-dom": "18.3.0"
  },
  "bugs": {
    "url": "https://github.com/samuelbillaud/tacos-ui/issues"
  },
  "homepage": "https://github.com/samuelbillaud/tacos-ui#readme"
}
