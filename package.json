{
  "name": "zaatar-ui",
  "version": "0.0.1",
  "packageManager": "pnpm@9.1.3",
  "private": true,
  "description": "The repository of zaatar-ui",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "packages": "pnpm --filter=./packages/** -recursive --parallel --stream",
    "build": "pnpm packages -- build",
    "dev": "pnpm --recursive --parallel --stream run dev",
    "dev:panda-preset": "pnpm --filter @tacos-ui/panda-preset run dev",
    "dev:components": "pnpm --filter @tacos-ui/react run dev",
    "format": "prettier --write packages",
    "format:check": "prettier --check packages",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Samuel Billaud <samuel.billaud1990@gmail.com> (https://github.com/samuelbillaud)",
  "repository": "https://github.com/samuelbillaud/zaatar-ui",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.10.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "dependencies": {
    "globals": "^15.0.0"
  }
}
